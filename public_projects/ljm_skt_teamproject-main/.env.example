# Environment Variables Example
# Copy this file to .env.local and update with your actual values

# API 설정
# Cloud Run 서비스 URL (기본값)
EXPO_PUBLIC_API_URL=https://nabiyam-chatbot-web-816056347823.asia-northeast3.run.app

# 워크스테이션 서버 URL (옵션 - ngrok URL 사용시)
# EXPO_PUBLIC_WORKSTATION_URL=https://your-ngrok-url.ngrok.io

# API 서비스 선택 (primary, secondary, workstation)
EXPO_PUBLIC_API_SERVICE=primary

# 개발 환경 설정
EXPO_PUBLIC_ENV=production

# 디버그 모드
EXPO_PUBLIC_DEBUG=false

# 타임아웃 설정 (밀리초)
EXPO_PUBLIC_API_TIMEOUT=15000

# 기타 설정
EXPO_PUBLIC_APP_NAME=나비얌
EXPO_PUBLIC_APP_VERSION=1.0.0